section.banner.bannerGtr
    .wrapper
        .bannerFlexContainer
            .bannerText
                h1.page__title-h1_big GTR chip
                h1.page__title-h1
                    span.page_red Make your car
                    | <br> stand out 
                p.pageText Performance tuning, on-board diagnostics, Acceleration and power meter, all in one device
                include ../socialIcons/socialIcons.component.pug
                
            .bannerImgBg
                picture 
                    source(type="image/png" srcset="assets/images/png/gtr/imgBannerGtrBg_1920.png" media="(min-width: 1441px)")
                    source(type="image/png" srcset="assets/images/png/gtr/imgBannerGtrBg_1440.png" media="(min-width: 1440px)")
                    source(type="image/png" srcset="assets/images/png/gtr/imgBannerGtrBg_992.png" media="(min-width: 992px)")
                    source( type="image/png" srcset="assets/images/png/gtr/imgBannerGtrBg_768.png" media="(min-width: 768px)")
                    img(src="assets/images/png/gtr/imgBannerGtrBg_320.png", alt="imgBannerGtrBg")
            .bannerImgChip
                picture 
                    source(type="image/png" srcset="assets/images/png/gtr/imgBannerGtrChipSm@1x.png 1x, assets/images/png/gtr/imgBannerGtrChipSm@2x.png 2x" media="(min-width: 768px)")
                    img(src="assets/images/png/gtr/imgBannerGtrChipXs@1x.png"  alt="imgBannerGtrChipXs")
                
            .bannerSelect
                include ../select/select.component.pug